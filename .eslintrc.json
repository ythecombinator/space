{
  "extends": ["next/core-web-vitals", "plugin:tailwindcss/recommended", "plugin:jsx-a11y/recommended"],
  "plugins": ["tailwindcss", "jsx-a11y", "unused-imports"],
  "rules": {
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "argsIgnorePattern": "^_" }
    ]
  }
}
